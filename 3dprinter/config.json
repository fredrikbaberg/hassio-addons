{
    "name": "3DPrinter",
    "version": "0.0.18",
    "slug": "3dprinter",
    "description": "3DPrinter add-on for Home Assistant. OctoPrint, Klipper and more.",
    "arch": ["armhf", "armv7", "amd64", "aarch64", "i386"],
    "url": "https://github.com/fredrikbaberg/hassio-addons",
    "startup": "application",
    "boot": "auto",
    "ports": {
        "5000/tcp": null,
        "5001/tcp": null,
        "5002/tcp": null,
        "7125/tcp": null,
        "8000/tcp": null
    },
    "ports_description": {
        "5000/tcp": "OctoPrint WebUI (Not required for Ingress)",
        "5001/tcp": "Fluidd",
        "5002/tcp": "Mainsail",
        "7125/tcp": "Moonraker",
        "8000/tcp": "Camera WebUI"
    },
    "devices": [
        "/dev/mem",
        "/dev/gpiomem"
    ],
    "homeassistant": "0.108.0",
    "privileged": [
        "SYS_RAWIO"
    ],
    "map": [],
    "environment": {},
    "video": true,
    "gpio": true,
    "usb": true,
    "uart": true,
    "options": {
        "mjpg_input": "-i \"input_uvc.so -n\"",
        "mjpg_output": "-o \"output_http.so -w /www_mjpg -p 8080\"",
        "recovery": false,
        "octoprint": true,
        "mjpg_streamer": false,
        "ingress_ui": "octoprint"
    },
    "schema": {
        "mjpg_input": "str",
        "mjpg_output": "str",
        "recovery": "bool",
        "octoprint": "bool",
        "mjpg_streamer": "bool",
        "klipper": "bool?",
        "moonraker": "bool?",
        "klipper_linux_host": "bool?",
        "fluidd": "bool?",
        "mainsail": "bool?",
        "ingress_ui": "list(octoprint|mainsail)"
    },
    "image": "fredrikbaberg/addon-3dprinter-{arch}",
    "tmpfs": true,
    "ingress": true,
    "panel_icon": "mdi:printer-3d",
    "panel_title": "3DPrinter",
    "panel_admin": false,
    "backup_exclude": [
        "*/*/fluidd",
        "*/*/klipper",
        "*/*/moonraker"
    ],
    "stage": "experimental",
    "realtime": true
}
