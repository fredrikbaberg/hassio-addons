{
    "name": "3DPrinter",
    "version": "2022.10.1",
    "slug": "3dprinter",
    "description": "3DPrinter add-on for Home Assistant. OctoPrint, Klipper and more.",
    "url": "https://github.com/fredrikbaberg/hassio-addons/tree/master/3dprinter",
    "arch": ["armhf", "armv7", "amd64", "aarch64", "i386"],
    "ports": {
        "5000/tcp": null,
        "7125/tcp": null,
        "8000/tcp": null
    },
    "ports_description": {
        "5000/tcp": "OctoPrint WebUI",
        "7125/tcp": "Moonraker",
        "8000/tcp": "Camera WebUI"
    },
    "devices": [
        "/dev/mem",
        "/dev/gpiomem"
    ],
    "privileged": [
        "SYS_RAWIO"
    ],
    "video": true,
    "gpio": true,
    "usb": true,
    "uart": true,
    "options": {
        "mjpg_input": "-i \"input_uvc.so -n\"",
        "mjpg_output": "-o \"output_http.so -w /www_mjpg -p 8080\"",
        "octoprint": true,
        "mjpg_streamer": false,
        "recovery": false
    },
    "schema": {
        "mjpg_input": "str",
        "mjpg_output": "str",
        "octoprint": "bool",
        "mjpg_streamer": "bool",
        "klipper": "bool?",
        "klipper_linux_host": "bool?",
        "moonraker": "bool?",
        "recovery": "bool"
    },
    "image": "ghcr.io/fredrikbaberg/addon-3dprinter-{arch}",
    "tmpfs": true,
    "ingress": true,
    "panel_icon": "mdi:printer-3d",
    "panel_title": "3DPrinter",
    "panel_admin": false,
    "backup_exclude": [
        "src/klipper",
        "src/moonraker",
        "python/klipper",
        "python/moonraker"
    ],
    "stage": "experimental",
    "init": false,
    "realtime": true
}
