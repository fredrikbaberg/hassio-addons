{
    "name": "3DPrinter",
    "version": "0.0.1",
    "slug": "3dprinter",
    "description": "3DPrinter add-on for Home Assistant. OctoPrint, Klipper and more.",
    "arch": ["armhf", "armv7", "amd64", "aarch64", "i386"],
    "url": "https://github.com/fredrikbaberg/hassio-addons",
    "startup": "application",
    "webui": "[PROTO:option_name]://[HOST]:[PORT:5000]",
    "boot": "auto",
    "ports": {
        "5000/tcp": null,
        "8000/tcp": null,
        "6000/tcp": null
    },
    "ports_description": {
        "5000/tcp": "OctoPrint WebUI (Not required for Ingress)",
        "8000/tcp": "Camera WebUI",
        "6000/tcp": "Fluidd"
    },
    "devices": [
        "/dev/mem",
        "/dev/gpiomem"
    ],
    "homeassistant": "0.108.0",
    "privileged": [
        "SYS_RAWIO"
    ],
    "map": [],
    "environment": {},
    "video": true,
    "gpio": true,
    "usb": true,
    "uart": true,
    "options": {
        "mjpg_arguments": [
            "-o \"output_http.so -w /www_mjpg -p 8080\"",
            "-i \"input_uvc.so -n\""
        ],
        "recovery": false,
        "ingress_ui": "list(octoprint|fluidd)",
        "autostart_mjpg_streamer": false,
        "autostart_klipper": false
    },
    "schema": {
        "mjpg_arguments": ["str"],
        "recovery": "bool",
        "autostart_octoprint": "bool?",
        "autostart_mjpg_streamer": "bool?",
        "autostart_klipper": "bool?",
        "autostart_moonraker": "bool?",
        "autostart_klipper_linux_host": "bool?"
    },
    "image": "fredrikbaberg/addon-3dprinter-{arch}",
    "tmpfs": true,
    "ingress": true,
    "panel_icon": "mdi:printer-3d",
    "panel_title": "3DPrinter",
    "panel_admin": false,
    "backup_exclude": [],
    "stage": "experimental",
    "realtime": true
}
