{
    "name": "PiOLED addon",
    "version": "2023.02.02",
    "slug": "pioled",
    "description": "Add-on with support for PiOLED",
    "arch": ["armhf", "armv7","aarch64"],
    "url": "https://github.com/fredrikbaberg/hassio-addons",
    "boot": "manual",
    "devices": [
        "/dev/mem",
        "/dev/gpiomem",
        "/dev/i2c-1"
