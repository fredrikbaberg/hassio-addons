    "homeassistant": "2021.4.0",
    "init": false,
    "privileged": [
        "SYS_RAWIO"
    ],
    "gpio": true,
    "uart": true,
    "stage": "experimental",
    "image": "ghcr.io/fredrikbaberg/addon-pioled-{arch}",
    "apparmor": false
}
