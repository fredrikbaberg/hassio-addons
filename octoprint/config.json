{
    "name": "Octoprint Experimental",
    "version": "0.3.0-alpha-8",
    "slug": "octoprint",
    "description": "Octoprint add-on for Hass.io",
    "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
    "url": "https://github.com/fredrikbaberg/hassio-addons",
    "startup": "application",
    "webui": "http://[HOST]:[PORT:5000]",
    "boot": "manual",
    "ports": {
        "5000/tcp": null
    },
    "ports_description": {
        "5000/tcp": "WebUI (Not required for Ingress)"
    },
    "auto_uart": true,
    "homeassistant": "0.108.0",
    "map": ["config:rw"],
    "options": {
        "reset_password": false
    },
    "schema": {
        "reset_password": "bool",
        "request_reset_data": "bool?"
    },
    "image": "fredrikbaberg/octoprint-{arch}-image",
    "ingress": true,
    "panel_icon": "mdi:printer-3d",
    "panel_title": "OctoPrint",
    "stage": "experimental"
}
