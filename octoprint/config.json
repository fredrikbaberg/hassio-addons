{
    "name": "Octoprint",
    "version": "0.3.0-alpha",
    "slug": "octoprint",
    "description": "Octoprint add-on for Hass.io",
    "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
    "url": "https://github.com/fredrikbaberg/hassio-addons",
    "startup": "application",
    "webui": "http://[HOST]:[PORT:5000]",
    "boot": "auto",
    "ports": {
        "5000/tcp": null
    },
    "ports_description": {
        "5000/tcp": "WebUI (Not required for Ingress)"
    },
    "auto_uart": true,
    "map": ["config:rw"],
    "options": {},
    "schema": {},
    "ingress": true,
    "panel_icon": "mdi:printer-3d",
    "panel_title": "OctoPrint",
    "stage": "experimental"
}
