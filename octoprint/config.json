{
    "name": "Octoprint",
    "version": "0.2.6",
    "slug": "octoprint",
    "description": "Octoprint add-on for Hass.io",
    "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
    "url": "https://github.com/fredrikbaberg/hassio-addons",
    "startup": "application",
    "boot": "manual",
    "ports": {
        "5000/tcp": 5000
    },
    "map": ["config:rw"],
    "options": {},
    "schema": {},
    "webui": "http://[HOST]:[PORT:5000]",
    "auto_uart": true,
    "image": "fredrikbaberg/octoprint-{arch}-image"
}
