{
    "name": "OctoPrint Next Dev",
    "version": "0.4.0",
    "slug": "octoprint-next-dev",
    "description": "OctoPrint add-on for Home Assistant",
    "arch": ["armv7", "amd64", "armhf", "aarch64", "i386"],
    "url": "https://github.com/fredrikbaberg/hassio-addons",
    "startup": "application",
    "webui": "[PROTO:option_name]://[HOST]:[PORT:5000]",
    "boot": "auto",
    "ports": {
        "5000/tcp": null,
        "8000/tcp": null
    },
    "ports_description": {
        "5000/tcp": "WebUI (Not required for Ingress)",
        "8000/tcp": "Camera web UI"
    },
    "devices": [
        "/dev/mem",
        "/dev/gpiomem"
    ],
    "homeassistant": "0.108.0",
    "privileged": [
        "SYS_RAWIO"
    ],
    "map": [],
    "environment": {},
    "video": true,
    "gpio": true,
    "usb": true,
    "uart": true,
    "options": {
        "mjpg_input": "input_uvc.so -n",
        "recovery": false
    },
    "schema": {
        "recovery": "bool",
        "mjpg_input": "str?"
    },
    "tmpfs": true,
    "ingress": true,
    "panel_icon": "mdi:printer-3d",
    "panel_title": "OctoPrint Next Dev",
    "panel_admin": false,
    "backup_exclude": [],
    "stage": "experimental"
}
