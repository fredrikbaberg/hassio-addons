{
    "name": "OctoPrint Klipper",
    "version": "0.7.4",
    "slug": "octoprint-klipper",
    "description": "OctoPrint with Klipper add-on for Home Assistant",
    "arch": ["armhf", "armv7", "amd64"],
    "url": "https://github.com/fredrikbaberg/hassio-addons",
    "startup": "application",
    "webui": "http://[HOST]:[PORT:5000]",
    "boot": "manual",
    "ports": {
        "5000": null,
        "8000": null
    },
    "ports_description": {
        "5000": "WebUI (Not required for Ingress)",
        "8000": "Camera web UI"
    },
    "devices": [
        "/dev/mem",
        "/dev/gpiomem"
    ],
    "homeassistant": "2021.4.0",
    "privileged": [
        "SYS_RAWIO"
    ],
    "map": [],
    "environment": {},
    "video": true,
    "gpio": true,
    "usb": true,
    "uart": true,
    "options": {
        "recovery": false,
        "mjpg_input": "input_uvc.so -n",
        "update_klipper": false
    },
    "schema": {
        "recovery": "bool",
        "mjpg_input": "str?",
        "update_klipper": "bool"
    },
    "image": "fredrikbaberg/addon-octoprint-klipper-{arch}",
    "tmpfs": true,
    "ingress": true,
    "panel_icon": "mdi:printer-3d-nozzle",
    "panel_title": "OctoPrint Klipper",
    "panel_admin": false,
    "snapshot_exclude": [],
    "stage": "experimental",
    "realtime": true
}
