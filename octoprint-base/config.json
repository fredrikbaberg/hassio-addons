{
    "name": "OctoPrint (base)",
    "version": "0.0.14",
    "slug": "octoprint-base",
    "description": "OctoPrint add-on for Home Assistant",
    "arch": ["armhf", "armv7", "amd64", "aarch64", "i386"],
    "url": "https://github.com/fredrikbaberg/hassio-addons",
    "startup": "application",
    "webui": "[PROTO:option_name]://[HOST]:[PORT:5000]",
    "boot": "auto",
    "ports": {
        "5000/tcp": null
    },
    "ports_description": {
        "5000/tcp": "WebUI (Not required for Ingress)"
    },
    "devices": [
        "/dev/mem",
        "/dev/gpiomem"
    ],
    "homeassistant": "0.108.0",
    "privileged": [
        "SYS_RAWIO"
    ],
    "map": [],
    "environment": {},
    "video": true,
    "gpio": true,
    "usb": true,
    "uart": true,
    "options": {
        "recovery": false
    },
    "schema": {
        "recovery": "bool"
    },
    "image": "fredrikbaberg/addon-octoprint-base-{arch}",
    "tmpfs": true,
    "ingress": true,
    "panel_icon": "mdi:printer-3d",
    "panel_title": "OctoPrint (base)",
    "panel_admin": false,
    "backup_exclude": [],
    "stage": "experimental"
}
